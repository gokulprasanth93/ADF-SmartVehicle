{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Dev-Azure-Factory001"
		},
		"ls_Adls_CovidReportingDL_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_Adls_CovidReportingDL'"
		},
		"ls_ablob_CovidReportingSA_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_CovidReportingSA'"
		},
		"ls_sql_sink_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sql_sink'"
		},
		"ls_Adls_CovidReportingDL_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storecovidadls.dfs.core.windows.net/"
		},
		"ls_http_opendata_ecdc_europa_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().BaseURL}"
		},
		"tr_event_trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/b075e2bf-4daa-474a-b220-636487adb9ce/resourceGroups/Dev-Azure005/providers/Microsoft.Storage/storageAccounts/blobstorecovid001"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Dev-Azure-Factory001_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_Adls_CovidReportingDL_accountKey": {
						"value": "[parameters('ls_Adls_CovidReportingDL_accountKey')]"
					},
					"ls_ablob_CovidReportingSA_connectionString": {
						"value": "[parameters('ls_ablob_CovidReportingSA_connectionString')]"
					},
					"ls_sql_sink_connectionString": {
						"value": "[parameters('ls_sql_sink_connectionString')]"
					},
					"ls_Adls_CovidReportingDL_properties_typeProperties_url": {
						"value": "[parameters('ls_Adls_CovidReportingDL_properties_typeProperties_url')]"
					},
					"ls_http_opendata_ecdc_europa_eu_properties_typeProperties_url": {
						"value": "[parameters('ls_http_opendata_ecdc_europa_eu_properties_typeProperties_url')]"
					},
					"tr_event_trigger_properties_typeProperties_scope": {
						"value": "[parameters('tr_event_trigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "Dev-Azure-Factory001_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'Dev-Azure-Factory001_ArmTemplate_0')]"
			]
		}
	]
}